$TTL	604800
{{ hostname }}.{{ domain }}.	IN	SOA	{{ hostname }}.{{ domain }}. admin.{{ hostname }}.{{ domain }}. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

	IN  NS			 vm2.{{ hostname }}.{{ domain }}.
	IN  A			 {{ dns_record[1].ip }}
{% for d in dns_record %}
{{ d.name }}   				 IN  {{ d.type }} {{ d.ip }}
{% endfor %}